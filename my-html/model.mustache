{{#models}}
    {{#model}}

        <h2>{{classname}}</h2>
            <table class="table table-striped table-bordered">
                <colgroup>
                    <col style="width: 10%;">
                    <col style="width: 30%;">
                    <col style="width: 5%;">
                    <col style="width: 5%;">
                    <col style="width: 20%;">
                    <col style="width: 30%;">
                </colgroup>
                <thead>
                <tr>
                    <th>Name</th>
                    <th>Description</th>
                    <th>Required</th>
                    <th>Schema</th>
                    <th>Default</th>
                    <th>Example</th>
                </tr>
                </thead>
                <tbody>
                {{#vars}}
                    <tr>
                        <td class="parameter"><p>{{name}}</p></td>
                        <td class="parameter"><p class="marked">{{description}}</p></td>
                        <td class="parameter">
                            {{#required}}<p>{{required}}</p>{{/required}}
                            {{^required}}<p>false</p>{{/required}}
                        </td>
                        <td class="parameter">
                            {{#baseType}}
                                <a href="#!/models#{{baseType}}">{{baseType}}</a>
                            {{/baseType}}
                        </td>
                        <td class="parameter"> <p class="marked">{{defaultValue}}</p></td>
                        <td class="parameter"> <p class="marked">{{example}}</p></td>
                    </tr>
                {{/vars}}
                </tbody>
            </table>
    {{/model}}
{{/models}}
